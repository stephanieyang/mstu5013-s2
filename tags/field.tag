<field>
    <div class="field-profile" id="field-{ field.attributes.name }">
      <h4><a href="{ field.attributes.links }" target="_blank">{ field.attributes.name }</a></h4>
      <p>{ field.attributes.desc }</p>
    </div>
    <style scoped>

    h4{
    margin-bottom: 0;
    }

    p {
    margin-top: 2px;
    margin-bottom: 25px;
    }
    </style>
</field>